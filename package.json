{
  "name": "effect_cqrs_eventsourcing_sample",
  "version": "1.0.0",
  "description": "University Course Registration and Academic Record Management System using Event Sourcing and CQRS with TypeScript Effect",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/main.ts",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest --watch --coverage",
    "test:coverage:ui": "vitest --ui --coverage",
    "test:coverage:open": "npm run test:coverage && open coverage/index.html",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["event-sourcing", "cqrs", "ddd", "typescript", "effect"],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@effect/schema": "^0.73.2",
    "effect": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.31.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}